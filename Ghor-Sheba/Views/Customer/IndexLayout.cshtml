<body class="cbp-spmenu-push">
    <div class="main-content">
        <!--left-fixed -navigation-->
        <div class=" sidebar" role="navigation">
            <div class="navbar-collapse">
                <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="/Customer/ViewBooking" class="active"><i class="fa fa-user nav_icon"></i>View Booking</a>
                        </li>

                        <li>
                            <a href="/Customer/MakeComplaint" class="active"><i class="fa fa-warning nav_icon"></i>Complaint Form</a>
                        </li>

                        <li>
                            <a href=# class="active"><i class="fa fa-user nav_icon"></i>Review</a>
                        </li>


                        <li>
                            <a href=/customer/viewcart class="active"><i class="fa fa-user nav_icon"></i>View Cart</a>
                        </li>

                    </ul>
                    <!-- //sidebar-collapse -->
                </nav>
            </div>
        </div>
        <!--left-fixed -navigation-->
        <!-- header-starts -->
        <div class="sticky-header header-section ">
            <div class="header-left">
                <!--toggle button start-->
                <button id="showLeftPush"><i class="fa fa-bars"></i></button>
                <!--toggle button end-->
                <!--logo -->
                <div class="logo">
                    <a href="/Home/Index">
                        <h1>Ghor Sheba</h1>
                        <span>Customer Panel</span>
                    </a>
                </div>
                <!--//logo-->
                <!--search-box-->
                <div class="clearfix"> </div>
            </div>
            <div class="header-right">
                <!--notification menu end -->
                <div class="profile_details">
                    <ul>
                        <li class="dropdown profile_details_drop">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                <div class="profile_img">
                                    <span class="prfil-img"><img src="~/icons/user.png" alt="" width="35px" height="35px"> </span>
                                    <div class="user-name">
                                        @{ var x = ViewData["username"];}
                                        <p>@x</p>
                                        <span>Customer</span>
                                    </div>
                                    <i class="fa fa-angle-down lnr"></i>
                                    <i class="fa fa-angle-up lnr"></i>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                            <ul class="dropdown-menu drp-mnu">
                                <li> <a href="/Login/Logout"><i class="fa fa-sign-out"></i> Logout</a> </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="clearfix"> </div>
        </div>
        <!-- //header-ends -->

        @RenderBody();

        <!--footer-->
    <div class="footer">
        <p>&copy; Ghor Sheba. All Rights Reserved</a></p>
    </div>
        <!--//footer-->
    </div>
    <!-- Classie -->
    <script src="~/CustomerTemplate/js/classie.js"></script>
    <script>
        var menuLeft = document.getElementById('cbp-spmenu-s1'),
            showLeftPush = document.getElementById('showLeftPush'),
            body = document.body;

        showLeftPush.onclick = function () {
            classie.toggle(this, 'active');
            classie.toggle(body, 'cbp-spmenu-push-toright');
            classie.toggle(menuLeft, 'cbp-spmenu-open');
            disableOther('showLeftPush');
        };


        function disableOther(button) {
            if (button !== 'showLeftPush') {
                classie.toggle(showLeftPush, 'disabled');
            }
        }
    </script>
    <!--scrolling js-->
    <script src="~/CustomerTemplate/js/jquery.nicescroll.js"></script>
    <script src="~/CustomerTemplate/js/scripts.js"></script>
    <!--//scrolling js-->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/CustomerTemplate/js/bootstrap.js"></script>
