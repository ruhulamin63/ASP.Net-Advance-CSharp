@model IEnumerable<Ghor_Sheba.Models.LoginUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Admin/IndexLayout.cshtml";
}
<title>Dashboard</title>
<!-- main content start-->

<div id="page-wrapper">
    <div class="main-page">
        <div class="row-one">
            <div class="col-md-4 widget">
                <div class="stats-left ">
                    <h5>Total</h5>
                    <h4>Managers</h4>
                </div>
                <div class="stats-right">
                    <label> @ViewData["managers"]</label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-mdl">
                <div class="stats-left">
                    <h5>Total</h5>
                    <h4>Customers</h4>
                </div>
                <div class="stats-right">
                    <label> @ViewData["customers"]</label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-last">
                <div class="stats-left">
                    <h5>Total</h5>
                    <h4>Workers</h4>
                </div>
                <div class="stats-right">
                    <label>@ViewData["sp"]</label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="clearfix"> </div>
        </div>
        <div class="row">
            <div class="col-md-8 stats-info stats-last widget-shadow">
                <table class="table table-bordered">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.username)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.email)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.user_type)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.status)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.username)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.email)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.user_type)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.status)
                            </td>
                        </tr>
                    }

                </table>
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
</div>



