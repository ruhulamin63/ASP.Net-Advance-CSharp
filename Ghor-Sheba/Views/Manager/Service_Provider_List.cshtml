@model IEnumerable<Ghor_Sheba.Models.ServiceProviderModel>

@{
    ViewBag.Title = "Service_Provider_List";
    Layout = "~/Views/Manager/IndexLayout.cshtml";
}

<div id="page-wrapper">
    <h2>Service Provider List</h2>
    <hr />
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.service_provider_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.service_provider_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.status)
                </td>
            </tr>
        }

    </table>
</div>