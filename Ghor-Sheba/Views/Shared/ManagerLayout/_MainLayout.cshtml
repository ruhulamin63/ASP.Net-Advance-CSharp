<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Manager</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/manager_assets/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/manager_assets/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    @RenderSection("Styles", required: false)
</head>

<body class="hold-transition sidebar-mini">
    @{ Html.RenderPartial("~/Views/Shared/ManagerLayout/_Navbar.cshtml");}
    @{ Html.RenderPartial("~/Views/Shared/ManagerLayout/_Sidebar.cshtml");}

    <div class="wrapper">
        @RenderBody()
    </div>

    @{ Html.RenderPartial("~/Views/Shared/ManagerLayout/_Footer.cshtml");}

    <!-- jQuery -->
    <script src="~/manager_assets/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/manager_assets/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/manager_assets/dist/js/adminlte.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
